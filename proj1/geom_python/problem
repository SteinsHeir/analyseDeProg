Main {
Constraint Y : {
    x >= 1;
    x <= 4;
    y >= 1;
    y <= 4;
    2y - x <= 6;
    2y - x >= 0;
    2x + y >= 4;
}
Constraint EX : {
    x >= 2;
    y >= 1;
    x + 2y >= 6;
    x - 2y >= -6;
}
Constraint CDD : {
    -x - y + 2 >= 0;
    x >= 0;
    y >= 0;
}
Constraint X1 : {
    x >= 4;
    y >= 3;
    x <= 4;
}
Constraint X2 : {
    x >= 4;
    y >= 3;
    x <= 3;
}
Constraint NotBounded : {
    x >= 0;
    x <= -1;
}
}